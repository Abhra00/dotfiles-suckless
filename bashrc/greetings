echo "┳┓┳┳┏┓┏┓  ┏━┓  ┳┓┳┳┓ ┓"
echo "┣┫┃┃┃┓┗┓  ┃┗┛  ┃┃┃┃┃ ┃"
echo "┻┛┗┛┗┛┗┛  ┗━┛  ┛┗┗┛┗┛┗┛"

if [[ $(tty) == *"pts"* ]]; then
    fastfetch -c $HOME/.config/fastfetch/config.jsonc
else
    if [ -f /bin/qtile ]; then
        echo "Start qtile with exec qtile start"
    fi
fi
